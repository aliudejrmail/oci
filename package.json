  "description": "Sistema de gestÃ£o de Ofertas de Cuidados Integrados (OCI) no SUS",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "cd client && npm install --include=dev && npm run build",
    "start": "node dist/server.js",
    "db:migrate": "prisma migrate dev --name init",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "importar:sigtap": "ts-node scripts/importar-compatibilidade-sigtap.ts",
