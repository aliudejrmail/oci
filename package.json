    "limpar:procedimentos-sem-sigtap": "ts-node scripts/limpar-procedimentos-sem-sigtap.ts",
    "verificar:oci-sigtap": "ts-node scripts/verificar-oci-sigtap.ts",
    "restaurar:procedimentos-oci": "ts-node scripts/restaurar-procedimentos-oci.ts",
